-- Scripts:

local function QDGA_fake_script() -- InertiaRoblox.openandcloseKey 
	local script = Instance.new('LocalScript', InertiaRoblox)

	local UIS = game:GetService("UserInputService")
	local tabs = script.Parent.tabs
	local logo = script.Parent.logo
	local main = script.Parent.main
	
	UIS.InputBegan:Connect(function(input)
		if input.KeyCode== Enum.KeyCode.RightShift then
			if tabs.Visible == true then
				tabs.Visible = false
				logo.Visible = false
				main.Visible = false
			elseif tabs.Visible == false then
				tabs.Visible = true
				logo.Visible = true
				main.Visible = true
			end
		end
	end)
end
coroutine.wrap(QDGA_fake_script)()
