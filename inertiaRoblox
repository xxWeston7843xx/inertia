local function ERXCL_fake_script() -- close.LocalScript 
	local script = Instance.new('LocalScript', close)

	local button = script.Parent
	local combat = script.Parent.Parent.combat
	local gamehub = script.Parent.Parent.gamehub
	local settings = script.Parent.Parent.settings
	local universal = script.Parent.Parent.universal
	
	function open()
		universal.Visible = true
		combat.Visible = true
		gamehub.Visible = true
		settings.Visible = true
	end
	
	function close()
		universal.Visible = false
		combat.Visible = false
		gamehub.Visible = false
		settings.Visible = false
	end
	
	button.MouseButton1Click:Connect(function()
		if universal.Visible == true then
			close()
		elseif universal.Visible == false then
			open()
		end
	end)
end
coroutine.wrap(ERXCL_fake_script)()
