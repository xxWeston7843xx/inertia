		singlerebirth.TextScaled = true
		singlerebirth.TextSize = 14.000
		singlerebirth.TextWrapped = true
	
		UICorner_6.Parent = singlerebirth
	
		openbutton.Name = "openbutton"
		openbutton.Parent = autofarm
		openbutton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		openbutton.Position = UDim2.new(0.0360219255, 0, 0.74242425, 0)
		openbutton.Size = UDim2.new(0, 153, 0, 42)
		openbutton.Visible = false
		openbutton.Font = Enum.Font.SourceSans
		openbutton.Text = "Open"
		openbutton.TextColor3 = Color3.fromRGB(255, 255, 255)
		openbutton.TextScaled = true
		openbutton.TextSize = 14.000
		openbutton.TextWrapped = true
	
		UICorner_7.Parent = openbutton
	
		-- Scripts:
	
		local function PTOEJB_fake_script() -- main.guihandler 
			local script = Instance.new('LocalScript', main)
	
			local speed = script.Parent.containerframe.speed
			local closebutton = script.Parent.topbar.closebutton
			local rebirth = script.Parent.containerframe.rebirth
			local singles = script.Parent.containerframe.singlespeed
			local singler = script.Parent.containerframe.singlerebirth
			local openbutton = script.Parent.Parent.openbutton
			local mainframe = script.Parent.Parent.main
			local UIS = game:GetService("UserInputService")
			local remotes = game:GetService("ReplicatedStorage").Remotes
	
			--Autospeed
			speed.MouseButton1Click:Connect(function()
				-- Turn Speed Autofarm On
				if speed.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
					speed.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
	
					while speed.BackgroundColor3 == Color3.fromRGB(0, 255, 0) do
						remotes.AddSpeed:FireServer()
						wait()
					end
					-- Turn Speed Autofarm Off
				elseif speed.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
					speed.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				end	
			end)
	
			--AutoRebirth
			rebirth.MouseButton1Click:Connect(function()
				--Turn RebirthAutoFarm On
				if rebirth.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
					rebirth.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
					remotes.Rebirth:FireServer()
					while rebirth.BackgroundColor3 == Color3.fromRGB(0, 255, 0) do
						remotes.Rebirth:FireServer()
						wait()
					end
					--Turn RebirthAutoFarmOff	
				elseif rebirth.BackgroundColor3 == Color3.fromRGB(0, 255, 0) then
					rebirth.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				end
			end)
	
			singles.MouseButton1Click:Connect(function()
				--Add Speed Once
				if singles.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
					singles.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
	
					remotes.AddSpeed:FireServer()
					wait(0.05)
					singles.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				end
			end)
	
			singler.MouseButton1Click:Connect(function()
				--Add Rebirth Once
				if singler.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
					singler.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
	
					remotes.Rebirth:FireServer()
					wait(0.05)
					singler.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				end
			end)
	
			--openandclose functions 
	
			--keybinds
			UIS.InputBegan:Connect(function(key)
				if key.KeyCode == Enum.KeyCode.LeftAlt then
					if mainframe.Visible == true then
						mainframe.Visible = false
						openbutton.Visible = true
	
					elseif key.KeyCode == Enum.KeyCode.LeftAlt then
						if mainframe.Visible == false then
							mainframe.Visible = true
							openbutton.Visible = false
						end
					end
				end
			end)
	
			closebutton.MouseButton1Click:Connect(function()
				mainframe.Visible = false
				openbutton.Visible = true
			end)
	
			openbutton.MouseButton1Click:Connect(function()
				mainframe.Visible = true
				openbutton.Visible = false
			end)
	
	
	
	
	
		end
		coroutine.wrap(PTOEJB_fake_script)()
		local function XFUYEN_fake_script() -- main.SmoothDrag 
			local script = Instance.new('LocalScript', main)
	
			--// _ Variables _ \\--
	
			local UserInputService = game:GetService("UserInputService")
	
			local SmoothDragToggle
			local SmoothDragInput
			local SmoothDragStart
	
			local SmoothDragSpeed = 0.1
	
			--// _ Smooth Drag _ \\--
	
			function SmoothDrag(Frame)
	
				local function UpdateSmoothDragInput(Input)
					local Delta = Input.Position - SmoothDragStart
					local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
					game:GetService("TweenService"):Create(Frame, TweenInfo.new(SmoothDragSpeed), {Position = Position}):Play()
				end
	
				Frame.InputBegan:Connect(function(Input)
					if (Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch) and UserInputService:GetFocusedTextBox() == nil then
						SmoothDragToggle = true
						SmoothDragStart = Input.Position
						startPos = Frame.Position
						Input.Changed:Connect(function()
							if Input.UserInputState == Enum.UserInputState.End then
								SmoothDragToggle = false
							end
						end)
					end
				end)
	
				Frame.InputChanged:Connect(function(Input)
					if Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch then
						SmoothDragInput = Input
					end
				end)
	
				game:GetService("UserInputService").InputChanged:Connect(function(Input)
					if Input == SmoothDragInput and SmoothDragToggle then
						UpdateSmoothDragInput(Input)
					end
				end)
	
			end
	
			SmoothDrag(script.Parent)
		end
		coroutine.wrap(XFUYEN_fake_script)()
	
	end)
end
coroutine.wrap(OKYKH_fake_script)()
